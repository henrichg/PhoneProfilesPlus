<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:key="eventRadioSwitchCategory"
    app:title="@string/event_preferences_category_eventRadioSwitchParameters" >

    <sk.henrichg.phoneprofilesplus.CustomSwitchPreference
        app:key="eventRadioSwitchEnabled"
        app:title="@string/event_preferences_enable_sensor"
        app:defaultValue="false"
        app:layout="@layout/mp_preference_material_widget"
        app:widgetLayout="@layout/preference_widget_custom_switch" />
    <sk.henrichg.phoneprofilesplus.InformationPreference
        app:key="eventRadioSwitchConfigInfo"
        app:title="@string/event_preferences_radioSwitch_info_title"
        app:summary="@string/event_preferences_radioSwitch_info_summary"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />
    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_wifi"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchWifi"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchWithConnectionArray"
        app:ppEntryValues="@array/eventRadioSwitchhWithConnectionValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />
    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_bluetooth"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchBluetooth"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchWithConnectionArray"
        app:ppEntryValues="@array/eventRadioSwitchhWithConnectionValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />

    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_simOnOff"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchSIMOnOff"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchSIMOnOffArray"
        app:ppEntryValues="@array/eventRadioSwitchSIMOnOffValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />

    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_defaultSIMForCalls"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchDefaultSIMForCalls"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchDefaultSIMArray"
        app:ppEntryValues="@array/eventRadioSwitchDefaultSIMValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />
    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_defaultSIMForSMS"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchDefaultSIMForSMS"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchDefaultSIMArray"
        app:ppEntryValues="@array/eventRadioSwitchDefaultSIMValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />

    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_mobileData"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchMobileData"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchWithConnectionArray"
        app:ppEntryValues="@array/eventRadioSwitchhWithConnectionValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />

    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_gps"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchGPS"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchArray"
        app:ppEntryValues="@array/eventRadioSwitchValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />
    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_nfc"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchNFC"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchArray"
        app:ppEntryValues="@array/eventRadioSwitchValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />
    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_airplaneMode"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchAirplaneMode"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchArray"
        app:ppEntryValues="@array/eventRadioSwitchValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />

    <sk.henrichg.phoneprofilesplus.PPListPreference
        app:title="@string/event_preferences_radioSwitch_ethernet"
        app:summary="@string/array_pref_radioSwitch_notDetect"
        app:key="eventRadioSwitchEthernet"
        app:defaultValue="0"
        app:ppEntries="@array/eventRadioSwitchWithConnectionArray"
        app:ppEntryValues="@array/eventRadioSwitchhWithConnectionValues"
        app:dependency="eventRadioSwitchEnabled"
        app:layout="@layout/mp_preference_material_widget" />

</androidx.preference.PreferenceScreen>