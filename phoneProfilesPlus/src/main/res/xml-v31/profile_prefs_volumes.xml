<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:title="@string/profile_preferences_category_volumes"
    app:key="prf_pref_volumeCategory" >

    <sk.henrichg.phoneprofilesplus.StartActivityPreference
        app:key="prf_pref_volumeUnlinkVolumesAppSettings"
        app:title="@string/phone_profiles_pref_applicationUnlinkRingerNotificationVolumes"
        app:summary="@string/info_notification_unlink_ringer_notification_volumes"
        app:layout="@layout/mp_preference_material_widget" />

    <sk.henrichg.phoneprofilesplus.CustomSwitchPreference
        app:key="prf_pref_volumeMuteSound"
        app:title="@string/profile_preferences_volumeMuteSound"
        app:summary="@string/profile_preferences_volumeMuteSound_summary"
        app:defaultValue="false"
        app:layout="@layout/mp_preference_material_widget"
        app:widgetLayout="@layout/preference_widget_custom_switch" />

    <PreferenceCategory
        app:key="prf_pref_volumeTypeCategory"
        app:title="@string/profile_preferences_category_volumeTypes"
        app:layout="@layout/mp_preference_category" >

        <Preference
            app:key="prf_pref_volumeSoundMode_info"
            app:title="@string/profile_preferences_configureSoundMode_info"
            app:summary=""
            app:layout="@layout/mp_preference_material_widget" />

        <sk.henrichg.phoneprofilesplus.InformationPreference
            app:key="prf_pref_volumeIgnoreSoundModeInfo2"
            app:title="@string/profile_preferences_volumeIgnoreSoundModeInfo_title"
            app:summary="@string/profile_preferences_volumeIgnoreSoundModeInfo_summary"
            app:layout="@layout/mp_preference_material_widget" />

        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeRingtone"
            app:forVolumesSensor="0"
            app:volumeType="ringtone"
            app:vNoChange="1"
            app:layout="@layout/mp_preference_material_widget"
            app:title="@string/profile_preferences_volumeRingtone"
            app:summary="@string/volume_pref_dialog_no_change_chb" />
        <sk.henrichg.phoneprofilesplus.InformationPreference
            app:key="prf_pref_volumeRingtone0Info"
            app:title="@string/profile_preferences_volumeRingtone0Info_title"
            app:summary="@string/profile_preferences_volumeRingtone0Info_summary"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeNotification"
            app:forVolumesSensor="0"
            app:volumeType="notification"
            app:vNoChange="1"
            app:layout="@layout/mp_preference_material_widget"
            app:title="@string/profile_preferences_volumeNotification"
            app:summary="@string/volume_pref_dialog_no_change_chb" />
        <sk.henrichg.phoneprofilesplus.NotificationVolume0DialogPreference
            app:key="prf_pref_volumeNotificationVolume0"
            app:title="@string/profile_preferences_volumeNotificationVolume0_title"
            app:summary="@string/profile_preferences_volumeNotificationVolume0_summaryConfigureForVolume0"
            app:layout="@layout/mp_preference_material_widget" />

        <sk.henrichg.phoneprofilesplus.InformationPreference
            app:key="prf_pref_volumeMediaInfo"
            app:title="@string/profile_preferences_volumeMedia"
            app:summary="@string/profile_preferences_volumeMediaInfo_summary"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.ExclamationPreference
            app:key="prf_pref_volumeMediaOnePlusInfo"
            app:title="@string/profile_preferences_volumeMedia"
            app:summary="@string/profile_preferences_volumeMediaOnePlusInfo_summary"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeMedia"
            app:forVolumesSensor="0"
            app:volumeType="media"
            app:vNoChange="1"
            app:title="@string/profile_preferences_volumeMedia"
            app:summary="@string/volume_pref_dialog_no_change_chb"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.CustomSwitchPreference
            app:key="prf_pref_volumeMediaChangeDuringPlay"
            app:title="@string/profile_preferences_volumeMediaChangeDuringPlay"
            app:defaultValue="false"
            app:layout="@layout/mp_preference_material_widget"
            app:widgetLayout="@layout/preference_widget_custom_switch" />

        <sk.henrichg.phoneprofilesplus.InformationPreference
            app:key="prf_pref_volumeAlarmInfo"
            app:title="@string/profile_preferences_volumeAlarm"
            app:summary="@string/profile_preferences_volumeAlarmInfo_summary"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeAlarm"
            app:forVolumesSensor="0"
            app:volumeType="alarm"
            app:vNoChange="1"
            app:title="@string/profile_preferences_volumeAlarm"
            app:summary="@string/volume_pref_dialog_no_change_chb"
            app:layout="@layout/mp_preference_material_widget" />

        <sk.henrichg.phoneprofilesplus.InformationPreference
            app:key="prf_pref_volumeMergedRingSyatemVolumeInfo"
            app:title="@string/profile_preferences_volumeSystem"
            app:summary="@string/profile_preferences_volumeMergedRingSyatemVolumeInfo_summary"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeSystem"
            app:forVolumesSensor="0"
            app:volumeType="system"
            app:vNoChange="1"
            app:title="@string/profile_preferences_volumeSystem"
            app:summary="@string/volume_pref_dialog_no_change_chb"
            app:layout="@layout/mp_preference_material_widget" />

        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeVoice"
            app:forVolumesSensor="0"
            app:volumeType="voice"
            app:vNoChange="1"
            app:title="@string/profile_preferences_volumeVoiceCall"
            app:summary="@string/volume_pref_dialog_no_change_chb"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeBluetoothSCO"
            app:forVolumesSensor="0"
            app:volumeType="bluetoothSco"
            app:vNoChange="1"
            app:title="@string/profile_preferences_volumeBluetoothSCO"
            app:summary="@string/volume_pref_dialog_no_change_chb"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeDTMF"
            app:forVolumesSensor="0"
            app:volumeType="dtmf"
            app:vNoChange="1"
            app:title="@string/profile_preferences_volumeDTMF"
            app:summary="@string/volume_pref_dialog_no_change_chb"
            app:layout="@layout/mp_preference_material_widget" />

        <sk.henrichg.phoneprofilesplus.ExclamationPreference
            app:key="prf_pref_volumeAccessibilityOnePlusInfo"
            app:title="@string/profile_preferences_volumeAccessibility"
            app:summary="@string/profile_preferences_volumeAccessibilityOnePlusInfo_31_summary"
            app:layout="@layout/mp_preference_material_widget" />
        <sk.henrichg.phoneprofilesplus.VolumeDialogPreference
            app:key="prf_pref_volumeAccessibility"
            app:forVolumesSensor="0"
            app:volumeType="accessibility"
            app:vNoChange="1"
            app:title="@string/profile_preferences_volumeAccessibility"
            app:summary="@string/volume_pref_dialog_no_change_chb"
            app:layout="@layout/mp_preference_material_widget" />

        <sk.henrichg.phoneprofilesplus.ExtenderDialogPreference
            app:title="@string/pppextender_pref_dialog_title"
            app:summary=""
            app:installSummary="@string/profile_preferences_volumeAccessibilityPrompt_PPPExtender_install_summary"
            app:enableExtenderSummaryDisabled="@string/profile_preferences_volumeAccessibilityPrompt_AccessibilitySettingsForExtender_summary"
            app:key="prf_pref_volumeAccessibilityExtender"
            app:layout="@layout/mp_preference_material_widget" />

    </PreferenceCategory>

</androidx.preference.PreferenceScreen>